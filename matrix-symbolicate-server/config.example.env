# Matrix 符号化服务配置文件示例
# 复制此文件为 .env 并修改配置

# 服务端口
PORT=8080

# 最大上传文件大小（字节）
MAX_UPLOAD_SIZE=524288000

# 符号化超时时间（秒）
SYMBOLICATE_TIMEOUT=5

# 日志级别 (debug, info, warn, error)
LOG_LEVEL=info

# 存储目录
DSYM_DIR=./dsyms
REPORTS_DIR=./reports
UPLOAD_DIR=./uploads

# 是否启用自动清理（天数）
AUTO_CLEANUP_DAYS=30

# 允许的跨域来源
CORS_ORIGINS=*

# 是否启用调试模式
DEBUG=false

